<h1>Hello, Rails...</h1>
<p>Let's start something usefull here.</p>

<%= link_to "New Project", new_project_path %>

<ul>
  <% @projects.each do |project| %>
    <li>
      <%= link_to project.title, project %> | <%= link_to "Edit", edit_project_path(project.id) %> | <%= link_to "Delete", project_path(project.id), data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"
                  } %>
    </li>
  <% end %>
</ul>