<%= form_with model: [ @project, @project.items.build ] do |form| %>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>
  <p>
    <%= form.label :body %><br>
    <%= form.text_area :body %>
  </p>
    <p>
    <%= form.select :status, ['open', 'in progress', 'resolved', 'archived'], selected: 'open' %>
  </p>
  <p>
    <%= form.submit %>
  </p>

<% end %>